<MudToolBar Style="@Style">
        <MudTooltip Text="@ExpandTooltip" Placement="Placement.Right">
            <MudIconButton Icon="@Icons.Material.Outlined.Menu" OnClick="@OnNavMenuStateChanged"/>
        </MudTooltip>
        <MudSpacer />
        <MudTooltip Text="通知">
            <MudIconButton Icon="@Icons.Material.Outlined.Notifications" />
        </MudTooltip>
        <MudTooltip Text="上传">
            <MudIconButton Icon="@Icons.Material.Outlined.FileUpload" />
        </MudTooltip>
        <MudTooltip Text="账号">
            <MudIconButton Icon="@Icons.Material.Outlined.ManageAccounts" />
        </MudTooltip>
</MudToolBar>

@code{
    [Parameter]
    public string Style { get; set; } = string.Empty;
}